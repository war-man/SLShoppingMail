
@{
    Layout = null;
    ViewBag.Title = "ServiceInfo";
}
<!doctype html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=9; IE=8; IE=7; IE=Edge" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1.0,minimum-scale=1.0" />
    <title>IM-DEMO</title>
    <style>
        .webim-dialog.webim-friend-requests {
            display: none;
        }

        .webim-logo {
            display: none;
        }

        .copyright{
            display:none;
        }

        .bg-color {
            border: 1px solid #008375 !important;
            background-color: #008375 !important;
        }

        input:focus, button:focus {
            border: 1px solid #008375 !important;
            box-shadow: rgba(0, 131, 117, 0.247059) 0px 0px 5px 0px !important;
        }

        .webim-sign p i {
            color: #008375 !important;
        }

        .webim-chatwindow-options > span:hover {
            color: #008375 !important;
        }

        .webim-operations-icon:hover, .webim-leftbar-icon:hover, .webim-operations-icon.selected, .webim-leftbar-icon.selected {
            color: #008375 !important;
        }
    </style>
    <!-- HTML5 element  -->
    <script src="../../../plugins/web-im/demo/javascript/dist/browser-polyfill.min.js"></script>
</head>
<body>
    <section id='main' class='w100'>
        <article id='demo'></article>
        <article id='components'></article>
    </section>
    <!--config-->
    <script src="../../../plugins/web-im/demo/javascript/dist/webim.config.js"></script>
    <script src="http://libs.baidu.com/jquery/2.0.0/jquery.js"></script>
    <script src="../../../plugins/web-im/sdk/dist/strophe-1.2.8.min.js"></script>
    <!--sdk-->
    <script src='../../../plugins/web-im/sdk/dist/websdk-1.4.13.js'></script>
    <!--webrtc-->
    <script>
        if (WebIM.config.isWebRTC) {
            document.write("<script src='../../../plugins/web-im/webrtc/dist/adapter.js'><\/script>");
            document.write("<script src='../../../plugins/web-im/webrtc/dist/webrtc-1.4.13.js'><\/script>");
        }
        var interval = setInterval(function () {
            if ($('.webim-dialog.webim-friend-requests').length != 0) {
                $('.webim-dialog.webim-friend-requests .webim-button.bg-color.webim-subscribe-button').trigger('click');
            }
        }, 500)
    </script>
    <!--[if lte IE 9]>
    <script src="../../../plugins/web-im/demo/javascript/dist/swfupload/swfupload.min.js"></script>
    <![endif]-->
    <!--demo javascript-->
    <script src="../../../plugins/web-im/demo/javascript/dist/demo-1.4.13.js"></script>
</body>
</html>



















